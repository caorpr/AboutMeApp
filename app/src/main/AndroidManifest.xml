<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ctec.aboutmeapp" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".AboutMeApp"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RunningActivity"
            android:label="@string/title_activity_running" >
        </activity>
        <activity
            android:name=".FoodActivity"
            android:label="@string/title_activity_food" >
        </activity>
        <activity
            android:name=".MusicActivity"
            android:label="@string/title_activity_music" >
        </activity>
        <activity
            android:name=".BoardGamesActivity"
            android:label="@string/title_activity_board_games" >
        </activity>
        <activity
            android:name=".VideoGamesActivity"
            android:label="@string/title_activity_video_games" >
        </activity>
    </application>

</manifest>
